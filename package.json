{
  "name": "barber-manager-desk",
  "productName": "Barbermanager",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./src/assets/img/barberLogo.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . Barbermanager --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./src/assets/img/barberLogo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Barbermanager\"",
    "package-linux": "electron-packager . Barbermanager --overwrite --asar=true --platform=linux --arch=x64 --icon=./src/assets/img/barberLogo.ico --prune=true --out=release-builds",
    "compodoc": "npx compodoc -p ./tsconfig.json -o -s --theme vagrant -w --customFavicon ./src/assets/img/barberLogo.ico --hideGenerator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "angular-material-fileupload": "^3.0.2",
    "electron-packager": "^15.2.0",
    "electron-reload": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~14.14.33",
    "codelyzer": "^6.0.0",
    "electron": "^12.0.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
